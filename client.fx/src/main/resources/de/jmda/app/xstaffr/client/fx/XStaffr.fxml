<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vbxRoot" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox fx:id="hbxTop" VBox.vgrow="NEVER" />
      <HBox fx:id="hbxCenter" VBox.vgrow="ALWAYS">
         <children>
            <StackPane fx:id="stckPn" HBox.hgrow="ALWAYS">
               <children>
              <AnchorPane fx:id="nchrPnSearchRequests" StackPane.alignment="CENTER" />
              <AnchorPane fx:id="nchrPnSearchResults" StackPane.alignment="CENTER" />
                <AnchorPane fx:id="nchrPnContracts" StackPane.alignment="CENTER" />
               </children>
            </StackPane>
         </children>
      </HBox>
      <HBox fx:id="hbxBottom" VBox.vgrow="NEVER" />
   </children>
</VBox>
