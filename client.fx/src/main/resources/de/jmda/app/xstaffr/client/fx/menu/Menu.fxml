            <MenuItem fx:id="mnItmEditRestore" mnemonicParsing="false" text="restore" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="mnItmExit" mnemonicParsing="false" text="exit" />
        </items>
      </Menu>
      <Menu fx:id="mnReport" mnemonicParsing="false" text="report">
        <items>
          <MenuItem fx:id="mnItemReportSearchRequests" mnemonicParsing="false" text="search requests" />
