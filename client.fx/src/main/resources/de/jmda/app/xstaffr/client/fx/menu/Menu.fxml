<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>

<MenuBar fx:id="menubar" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
  <menus>
      <Menu fx:id="mnEdit" mnemonicParsing="false" text="edit">
        <items>
          <MenuItem fx:id="mnItmEditCandidates" mnemonicParsing="false" text="candidates" />
            <MenuItem fx:id="mnItmEditCustomers" mnemonicParsing="false" text="customers" />
            <MenuItem fx:id="mnItmEditRequesters" mnemonicParsing="false" text="requesters" />
